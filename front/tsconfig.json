{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 10",

  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "outDir": "dist",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedParameters": true,
    "jsx": "preserve",
    "esModuleInterop": true
  }
}